<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Chef</title>
    <!-- Bootstrap core CSS     -->
    <link href="~/RecursosAdmin/Administrador/css/bootstrap.min.css" rel="stylesheet" />
    <!-- Animation library for notifications   -->
    <link href="~/RecursosAdmin/Administrador/css/animate.min.css" rel="stylesheet" />
    <!--  Light Bootstrap Table core CSS    -->
    <link href="~/RecursosAdmin/Administrador/css/light-bootstrap-dashboard.css" rel="stylesheet" />
    <!--CSS for Demo Purpose, don't include it in your project     -->
    <link href="~/RecursosAdmin/Administrador/css/demo.css" rel="stylesheet" />
    <link href="~/RecursosAdmin/Administrador/css/Imagen.css" rel="stylesheet" />

    <!--Fonts and icons-->
    <link href="http://maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css" rel="stylesheet">
    <link href='http://fonts.googleapis.com/css?family=Roboto:400,700,300' rel='stylesheet' type='text/css'>
    <!--<link href="~/RecursosAdmin/Administrador/css/pe-icon-7-stroke.css" rel="stylesheet" /-->
    <style>
        input[type="file"]::-webkit-file-upload-button {
            color: white;
            border: 1px solid green;
            border-radius: 5px;
            background: orange;
        }
    </style>

</head>
<body>


    <div class="wrapper">
        <div class="sidebar" data-color="orange" style="background-color:#ffb100">


            <div class="sidebar-wrapper">
                <div class="logo">
                    <a href="http://www.creative-tim.com" class="simple-text">
                        COCINA TECH
                    </a>
                </div>
                <ul class="nav">
                    <li id="menu-usuario">
                        <a href="~/DashBoardAprobado/DashBoardAprobado">
                            <img src="~/Renove/images/003-checked-files.png" width="38" style="float: left;margin-right: 15px;line-height: 30px;text-align: center;" />
                            <p>Pedidos Aprobados</p>
                        </a>
                    </li>
                    <li id="menu-registrousuarios">
                        <a href="~/DashBoard/DashBoard">
                            <img src="~/Renove/images/001-to-do-list.png" width="38" style="float: left;margin-right: 15px;line-height: 30px;text-align: center;" />
                            <p>Pedidos por Aprobar</p>
                        </a>
                    </li>
                    
                </ul>
            </div>
        </div>

        <div class="main-panel">
            <nav class="navbar navbar-default navbar-fixed">
                <div class="container-fluid">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#navigation-example-2">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                        <a class="navbar-brand" href="#">Chef</a>
                    </div>
                    <div class="collapse navbar-collapse">
                        <ul class="nav navbar-nav navbar-left">
                            <li>
                                <a href="#" class="dropdown-toggle" data-toggle="dropdown">

                                    <p class="hidden-lg hidden-md">Chef</p>
                                </a>
                            </li>
                            <li class="dropdown">
                                <a href="#" class="dropdown-toggle" data-toggle="dropdown">

                                    <b class="caret hidden-lg hidden-md"></b>
                                    <p class="hidden-lg hidden-md">
                                        @ViewBag.Count
                                        <b class="caret"></b>
                                    </p>
                                </a>

                            </li>
                        </ul>

                        <ul class="nav navbar-nav navbar-right">

                            <li class="dropdown">
                                <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                    <p>
                                        @Session["Nombre_usu"]
                                        <b class="caret"></b>
                                    </p>
                                </a>
                                <ul class="dropdown-menu">
                                    <!--li><a href="#">Something</a></!--li-->
                                    <!--li class="divider"></!--li>-->
                                    <li><a href="~/Principal/InicioPrincipal">Cerrar Sesion</a></li>
                                </ul>
                            </li>

                            <li class="separator hidden-lg"></li>
                        </ul>
                    </div>
                </div>
            </nav>



            <section>
                @RenderBody()
            </section>
        </div>
    </div>
    <section>
        <div class="container">
            <div class="row">
                <div class="col-sm-12">
                    <div id="Modalxd" class="modal fade" role="dialog">
                        <div class="modal-dialog" role="document">
                            <div class="modal-content">
                                <div class="modal-header">
                                    <h5 class="modal-title" id="exampleModalLabel">Ingredientes</h5>
                                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                        <span aria-hidden="true">&times;</span>
                                    </button>
                                </div>
                                <div class="modal-body">

                                    @{
                                        Html.RenderAction("Agregaringredentespatillo", "RegistroPlatillo");
                                    }


                                </div>
                                <div class="modal-footer">
                                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Cerrar</button>
                                    <button type="button" class="btn btn-primary">Guarda ingredientes</button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>


    </section>

    <script src="~/RecursosAdmin/js/jquery-1.11.2.min.js"></script>
    <script src="~/RecursosAdmin/Administrador/js/bootstrap.min.js"></script>
    <!--  Charts Plugin -->
    <script src="~/RecursosAdmin/Administrador/js/chartist.min.js"></script>
    <!--  Notifications Plugin    -->
    <script src="~/RecursosAdmin/Administrador/js/bootstrap-notify.js"></script>
    <!--  Google Maps Plugin    -->
    <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=YOUR_KEY_HERE"></script>
    <!-- Light Bootstrap Table Core javascript and methods for Demo purpose -->
    <script src="~/RecursosAdmin/Administrador/js/light-bootstrap-dashboard.js"></script>
    <!-- Light Bootstrap Table DEMO methods, don't include it in your project! -->
    <script src="~/RecursosAdmin/Administrador/js/demo.js"></script>
    <script src="https://cdn.datatables.net/1.10.16/js/jquery.dataTables.min.js"></script>
    <script>
        $(document).ready(function () {
            if (window.location.href.indexOf("InicioAdmin") > -1) {
                $(".sidebar-wrapper li").removeClass("active");
                $("#menu-usuario").addClass("active");
            }
            else if (window.location.href.indexOf("RegistroUsuarios") > -1) {
                $(".sidebar-wrapper li").removeClass("active");
                $("#menu-registrousuarios").addClass("active");
            }
            else if (window.location.href.indexOf("AgregarChef") > -1) {
                $(".sidebar-wrapper li").removeClass("active");
                $("#menu-registromonitor").addClass("active");
            }

            else if (window.location.href.indexOf("RegistarCategorias") > -1) {
                $(".sidebar-wrapper li").removeClass("active");
                $("#menu-registrocategoria").addClass("active");
            }
            else if (window.location.href.indexOf("ListadoGeneral") > -1) {
                $(".sidebar-wrapper li").removeClass("active");
                $("#menu-registropedido").addClass("active");
            }
            else if (window.location.href.indexOf("IngredienteM") > -1) {
                $(".sidebar-wrapper li").removeClass("active");
                $("#menu-registromedida").addClass("active");
            }
            else if (window.location.href.indexOf("VistaIngrediente") > -1) {
                $(".sidebar-wrapper li").removeClass("active");
                $("#menu-registroingredientes").addClass("active");
            }
            else if (window.location.href.indexOf("Platillopingrediente") > -1) {
                $(".sidebar-wrapper li").removeClass("active");
                $("#menu-ingredientesparaplatillo").addClass("active");
            }
            else if (window.location.href.indexOf("InboxAdmin") > -1) {
                $(".sidebar-wrapper li").removeClass("active");
                $("#menu-mensajes").addClass("active");
            }
            else if (window.location.href.indexOf("InboxAdmin") > -1) {
                $(".sidebar-wrapper li").removeClass("active");
                $("#menu-mensajes").addClass("active");
            }
            else if (window.location.href.indexOf("RegistroPlatillo") > -1) {
                $(".sidebar-wrapper li").removeClass("active");
                $("#menu-registroplatillo").addClass("active");
            }
        });
    </script>

